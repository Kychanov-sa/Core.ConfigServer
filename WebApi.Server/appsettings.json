{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Diagnostics": {
    "EnableRequestProfiling": false,
    "EnableSettingsLogging": true,
    "EnableAuditLogging": true,
    "EnableExtendedLogOutput": false
  },

  "ConnectionStrings": {
    "Database": "Data source=mongodb://localhost:27017;Initial catalog=ConfigServer_Dev;"
  }
}
